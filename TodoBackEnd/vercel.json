{
  "public": true,
  "routes": [
    {
      "src": "/tasks",
      "dest": "index.js"
    },
    {
      "src": "/tasks/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/profile",
      "dest": "index.js"
    },
    {
      "src": "/profile/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/auth",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/auth/signup",
      "dest": "public/index.html"
    },
    {
      "src": "/auth/login",
      "dest": "public/index.html"
    },
    {
      "src": "/user/(.*)",
      "dest": "public/index.html"
    }
  ]
}
